<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cl.androidstudy">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.me.author.AuthorActivity"></activity>
        <activity android:name=".ui.me.permission.GithubActivity" />
        <activity android:name=".ui.me.integralrank.IntegralRankActivity" />
        <activity android:name=".ui.me.integralme.MyIntegralActivity" />
        <activity android:name=".ui.me.share.ShareActivity" />
        <activity android:name=".ui.me.collection.CollectionActivity" />
        <activity android:name=".ui.me.login.LoginActivity" />
        <activity android:name=".ui.home.search.SearchActivity" />
        <activity android:name=".ui.web.WebActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>